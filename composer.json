{
    "name": "dzentota/session",
    "description": "Secure session management library with a focus on security and type safety",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Tatulchenkov",
            "email": "webtota@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/simple-cache": "^1.0",
        "ramsey/uuid": "^4.0",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Dzentota\\Session\\": "src/Session/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dzentota\\Session\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
